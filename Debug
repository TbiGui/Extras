		Enabled = true,
		FolderName = "TBIGUISAVECONFIGS",
		FileName = "TBIGUISAVES"
	},
})

function Notification(Text)
	Rayfield:Notify({
		Title = "Error",
		Content = Text,
		Duration = 3,
		Image = nil,
	})
end

function InfoNotification(Text)
	Rayfield:Notify({
		Title = "Info",
		Content = Text,
		Duration = 3,
		Image = nil,
	})
end

