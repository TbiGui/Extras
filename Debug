	Name = "TbiGui Window - Beta",
	Icon = 116531676650470, 
	LoadingTitle = "TbiGui Interface",
	LoadingSubtitle = "by tbi",
	Theme = "Default",

	DisableRayfieldPrompts = true,
	DisableBuildWarnings = false,

